ow.localStorage\n});\n\nexport default new Vuex.Store({\n    state: {\n        ausbildung: [\n            {sid: 100, mid: mathe, title: 'Mathematik', mark: 4.0},\n            {\n                sid: 100, mid: 287, title: 'Deutsch', mark: 3.5,\n                _cellVariants: {mark: 'danger'}\n            },\n            {sid: 100, mid: 288, title: 'Finanz- und Rechnungswesen', mark: 5.5},\n            {sid: 100, mid: 307, title: 'Englisch', mark: 5.5},\n            {sid: 100, mid: 290, title: 'Wirtschaft und Recht', mark: 3.5,\n                _cellVariants: {mark: 'danger'}\n            },\n            {sid: 100, mid: 291, title: 'Französisch', mark: 'Pnab',\n                _cellVariants: {mark: 'warning'}\n            },\n            {sid: 101, mid: mathe, title: 'Mathematik', mark: 5},\n            {sid: 101, mid: 287, title: 'Deutsch', mark: 5.0 },\n            {sid: 101, mid: 288, title: 'Finanz- und Rechnungswesen', mark: 5},\n            {sid: 101, mid: 307, title: 'Englisch', mark: 6},\n            {sid: 101, mid: 290, title: 'Wirtschaft und Recht', mark: 6.0},\n            {sid: 101, mid: 291, title: 'Französisch', mark: 4,\n                _cellVariants: {mark: 'warning'}\n            }\n        ]\n\n    },\n    getters: {\n        getByStudentId(state) {\n            return function (sid) {\n                return state.ausbildung.filter(mod => {\n                    if (mod.sid === sid) {\n                        //console.log(mod);\n                        return mod;\n                    }\n                });\n            };\n        }\n    },\n    mutations: {\n        updateMark(state,modList){\n            state.ausbildung = modList;\n        }\n    },\n    actions: {},\n    modules: {},\n    plugins: [vuexLocal.plugin]\n})\n"]}]}